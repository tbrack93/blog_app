<%= link_to "New Article", new_article_path %>

<% if @articles.empty? %>
  <h3 id="no-articles"> No Articles Created </h3>
<%end%>
<% @articles.each do |article|%>
  <div class="col-xs-8 col-xs-offset-4">
    <h3><%= link_to article.title, article_path(article)%></h3>
    <p><%= article.body %></p>
  </div>
<%end%>
